# my git aliases
alias gt='git'
alias gtc='git checkout'
alias gtp='git pull'
alias gth='git push'
alias gts='git status'
alias gtb='git branch'
alias gtd='git diff'
alias gtf='git fetch'
alias gtline='git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit'
alias gtlf='gti log --stat'

# my ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias lt='ls -lrthF'

# my general aliases
alias cl='clear'
